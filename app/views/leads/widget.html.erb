  <%= simple_form_for @lead, html: { method: :post, class: :'form' } do |f| %>
  <%= f.input :first_name, required: false %>
  <%= f.input :last_name %>
  <%= f.input :email %>
  <div class="form-group">
  <%= f.input :address, class:'form-control' %>
  </div>
  <div class="form-group">
  <%= f.input :address_2, class:'form-control' %>
  </div>
  <div class="form-group">
  <%= f.input :zip, class:'form-control' %>
  </div>
  <div class="form-group">
  <%= f.input :city, class:'form-control' %>
  </div>
  <%= f.input :company %>
  <%= f.button :submit, class:'btn btn-warning' %>
  <%= hidden_field_tag(:user_id, "1") %>
<% end %>

